<?xml version="1.0"?>

<PropertyList>

	<path>Compass.ac</path>
	<animation>
		<type>material</type>
		<object-name>Ring</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission> 
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Ring</object-name>
		<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
        <axis>
        	<x1-m>-0.0116</x1-m>
            <y1-m> 0.0000</y1-m>
            <z1-m>-0.0040</z1-m>
            <x2-m>-0.0072</x2-m>
            <y2-m> 0.0000</y2-m>
            <z2-m> 0.0122</z2-m>
        </axis>
	</animation>

</PropertyList>