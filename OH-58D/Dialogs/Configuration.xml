<?xml version="1.0"?><PropertyList>	<name>Configuration</name>	<layout>vbox</layout>	<text>		<label>Configuration</label>		<valign>center</valign>		<halign>center</halign>	</text>	<hrule/>	<group>		<layout>hbox</layout>		<halign>center</halign>		<valign>top</valign>		<group>			<layout>vbox</layout>			<halign>left</halign>			<valign>top</valign>			<checkbox>				<halign>left</halign>				<label>Pylons</label>				<property>sim/model/livery/pylons</property>				<binding>					<command>property-toggle</command>					<property>sim/model/livery/pylons</property>				</binding>			</checkbox>			<checkbox>				<halign>left</halign>				<label>Doors</label>				<property>sim/model/livery/doors</property>				<binding>					<command>property-toggle</command>					<property>sim/model/livery/doors</property>				</binding>			</checkbox>			<checkbox>				<halign>left</halign>				<label>Plates</label>				<property>sim/model/livery/plates</property>				<binding>					<command>property-toggle</command>					<property>sim/model/livery/plates</property>				</binding>			</checkbox>			<checkbox>				<halign>left</halign>				<label>Filters</label>				<property>sim/model/livery/filters</property>				<binding>					<command>property-toggle</command>					<property>sim/model/livery/filters</property>				</binding>			</checkbox>		</group>	</group>	<hrule/>	<group>		<layout>hbox</layout>		<empty>			<stretch>true</stretch>		</empty>		<button>			<legend>Close</legend>			<default>true</default>			<key>Esc</key>			<binding>				<command>dialog-close</command>			</binding>		</button>		<empty>			<stretch>true</stretch>		</empty>	</group></PropertyList>